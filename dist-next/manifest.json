{
  "name": "Core NextGen",
  "short_name": "Core NextGen",
  "version": "0.0.0",
  "manifest_version": 3,
  "author": "AvaLabs",
  "description": "Your home for Avalanche (AVAX) and beyond.",
  "default_locale": "en",
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Alt+Shift+B",
        "mac": "Alt+Shift+B",
        "chromeos": "Alt+Shift+B",
        "linux": "Alt+Shift+B"
      }
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png",
    "192": "images/icon-192.png"
  },
  "background": {
    "service_worker": "sw/js/backgroundPage.js"
  },
  "action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png"
    },
    "default_title": "Core NextGen Browser Extension",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "activeTab",
    "contextMenus",
    "notifications",
    "tabs",
    "scripting",
    "identity",
    "alarms",
    "offscreen"
  ],
  "host_permissions": [
    "file://*/*",
    "http://*/*",
    "https://*/*"
  ],
  "oauth2": {
    "client_id": "1023766203719-o6q337dkc3brnumrkmsjuq8vvvd06p9s.apps.googleusercontent.com",
    "scopes": [
      "openid",
      "email"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "file://*/*",
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "cs/js/contentscript.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'self'; script-src 'self' 'wasm-unsafe-eval'; img-src 'self' https: ipfs: data:; connect-src 'self' http: https: wss:; font-src 'self' data: https://fonts.gstatic.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; child-src 'none'; frame-ancestors 'none'; object-src 'none'"
  }
}