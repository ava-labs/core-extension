{
  "name": "@core/inpage",
  "packageManager": "yarn@4.7.0",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "dev": "rsbuild build -w --config rsbuild.inpage.ts",
    "dev:next": "rsbuild build -w --config rsbuild.inpage.ts -- --core-gen=next",
    "build": "rsbuild build --config rsbuild.inpage.ts",
    "build:next": "rsbuild build --config rsbuild.inpage.ts -- --core-gen=next",
    "lint": "eslint --fix \"src/**/*.ts*\"",
    "prettify": "prettier --write \"src/**/*.ts*\"",
    "test": "yarn jest",
    "test:watch": "yarn jest --watch",
    "typecheck": "yarn tsc --skipLibCheck --noEmit"
  },
  "dependencies": {
    "@avalabs/evm-module": "1.10.0",
    "@avalabs/svm-module": "1.10.0",
    "@core/common": "workspace:*",
    "@core/messaging": "workspace:*",
    "@core/types": "workspace:*",
    "eth-rpc-errors": "4.0.3",
    "events": "3.3.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.4",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.16.0",
    "@lavamoat/allow-scripts": "2.0.0",
    "@lavamoat/preinstall-always-fail": "1.0.0",
    "@rsbuild/core": "1.3.3",
    "@rsbuild/plugin-node-polyfill": "1.3.0",
    "@rsbuild/plugin-react": "1.1.1",
    "@rsdoctor/rspack-plugin": "1.0.2",
    "@rspack/core": "1.3.2",
    "@typescript-eslint/eslint-plugin": "8.17.0",
    "@typescript-eslint/parser": "8.17.0",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "ethers": "6.13.5",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.3.1",
    "prettier": "3.4.2",
    "ts-jest": "29.3.0",
    "typescript": "5.8.2"
  }
}
