{
  "name": "avalanche-extension",
  "version": "0.0.0",
  "license": "Limited Ecosystem License",
  "private": true,
  "scripts": {
    "start": "yarn dev:legacy",
    "dev": "yarn dev:legacy",
    "dev:next": "yarn workspaces foreach --include '@core/*' --include @core-ext/next -Api run dev:next",
    "dev:legacy": "yarn workspaces foreach --include '@core/*' --include @core-ext/legacy -Api run dev",
    "build:packages": "yarn workspaces foreach -Api --include '@core/*' run build",
    "build:packages:next": "yarn workspaces foreach -Api --include '@core/*' run build:next",
    "build:legacy": "yarn build:packages && yarn workspace @core-ext/legacy run build",
    "build:legacy:alpha": "yarn build:packages && yarn workspace @core-ext/legacy run build:alpha",
    "build:next": "yarn build:packages:next && yarn workspace @core-ext/next run build",
    "build:next:alpha": "yarn build:packages:next && yarn workspace @core-ext/next run build:alpha",
    "build": "yarn build:legacy",
    "build:alpha": "yarn build:legacy:alpha",
    "build:alpha:no-source-maps": "NO_SOURCE_MAPS=true yarn build:alpha",
    "lint": "yarn workspaces foreach -Ap --exclude @core-ext/next run lint",
    "lint:next": "yarn workspaces foreach -Ap --exclude @core-ext/legacy run lint",
    "postinstall": "husky install",
    "prettify": "yarn workspaces foreach -Ap run prettify",
    "typecheck": "yarn workspaces foreach -Ap --exclude @core-ext/next run typecheck",
    "typecheck:next": "yarn workspaces foreach -Ap --exclude @core-ext/legacy run typecheck",
    "setup": "yarn install && yarn allow-scripts",
    "test": "yarn workspaces foreach -Ap --exclude @core-ext/next run test",
    "test:next": "yarn workspaces foreach -Ap --exclude @core-ext/legacy run test",
    "test:watch": "yarn workspaces foreach -Ap run test:watch",
    "test:path": "yarn jest --watch --testPathPattern",
    "zip": "node build-scripts/zipscript.mjs --core-gen=legacy",
    "zip:next": "node build-scripts/zipscript.mjs --core-gen=next",
    "scanner": "yarn scanner:legacy",
    "scanner:next": "yarn workspace @core-ext/next run scanner",
    "scanner:legacy": "yarn workspace @core-ext/legacy run scanner",
    "sentry": "node sentryscript.js",
    "submit-build": "node build-scripts/submitBuild.mjs"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "devDependencies": {
    "@eslint/compat": "1.2.4",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.16.0",
    "@lavamoat/allow-scripts": "2.0.0",
    "@lavamoat/preinstall-always-fail": "1.0.0",
    "@rsbuild/core": "1.3.3",
    "@rsbuild/plugin-node-polyfill": "1.3.0",
    "@rsbuild/plugin-react": "1.1.1",
    "@rsdoctor/rspack-plugin": "1.0.2",
    "@rspack/core": "1.3.2",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/github": "8.0.2",
    "@typescript-eslint/eslint-plugin": "8.17.0",
    "@typescript-eslint/parser": "8.17.0",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "husky": "7.0.4",
    "prettier": "3.4.2",
    "semantic-release": "19.0.3",
    "typescript": "5.8.2"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "bracketSameLine": false
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{ts,tsx}": "eslint --fix --max-warnings 0"
  },
  "volta": {
    "node": "20.18.0",
    "yarn": "4.7.0"
  },
  "lavamoat": {
